{
  "compilerOptions": {
    "baseUrl": "./src",
    "composite": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components": [
        "./components"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@modules/*": [
        "./modules/*"
      ]
    }
  }
}